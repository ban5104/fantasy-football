M RANKINGS\\\\n\\\")\\n\",\n    \"            \\n\",\n    \"            # Format for display\\n\",\n    \"            comparison['Total_VBD'] = comparison['Total_VBD'].round(1)\\n\",\n    \"            comparison['Avg_VBD'] = comparison['Avg_VBD'].round(1)\\n\",\n    \"            comparison['Value_Gained'] = comparison['Value_Gained'].round(1)\\n\",\n    \"            \\n\",\n    \"            display(HTML(comparison.to_html(index=False, classes='table table-striped')))\\n\",\n    \"            \\n\",\n    \"            # Create visualization\\n\",\n    \"            fig = px.scatter(\\n\",\n    \"                comparison,\\n\",\n    \"                x='Total_VBD',\\n\",\n    \"                y='Value_Gained',\\n\",\n    \"                hover_data=['Team', 'Grade'],\\n\",\n    \"                title=\\\"Team Performance: Total VBD vs Value Gained\\\",\\n\",\n    \"                labels={'Total_VBD': 'Total Value Above Replacement', 'Value_Gained': 'Draft Value Gained'}\\n\",\n    \"            )\\n\",\n    \"            \\n\",\n    \"            fig.update_traces(marker=dict(size=12))\\n\",\n    \"            fig.update_layout(height=500)\\n\",\n    \"            fig.show()\\n\",\n    \"\\n\",\n    \"analyze_button.on_click(analyze_selected_team)\\n\",\n    \"compare_button.on_click(compare_all_teams)\\n\",\n    \"\\n\",\n    \"print(\\\"üîç TEAM ANALYSIS TOOLS\\\")\\n\",\n    \"display(widgets.VBox([\\n\",\n    \"    widgets.HBox([team_selector, analyze_button, compare_button]),\\n\",\n    \"    analysis_output\\n\",\n    \"]))\"]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# Draft Visualization Dashboard\\n\",\n    \"def create_draft_dashboard():\\n\",\n    \"    \\\"\\\"\\\"Create comprehensive draft visualization dashboard\\\"\\\"\\\"\\n\",\n    \"    \\n\",\n    \"    if not analyzer.draft_data:\\n\",\n    \"        print(\\\"‚ùå Please load draft results first\\\")\\n\",\n    \"        return\\n\",\n    \"    \\n\",\n    \"    # Convert draft data to DataFrame\\n\",\n    \"    draft_df = pd.DataFrame(analyzer.draft_data)\\n\",\n    \"    \\n\",\n    \"    # Create subplots\\n\",\n    \"    fig = make_subplots(\\n\",\n    \"        rows=2, cols=2,\\n\",\n    \"        subplot_titles=[\\n\",\n    \"            'VBD by Round',\\n\",\n    \"            'Position Distribution',\\n\",\n    \"            'Team Value Accumulation',\\n\",\n    \"            'Pick Value vs Expected'\\n\",\n    \"        ],\\n\",\n    \"        specs=[[{'secondary_y': False}, {'type': 'domain'}],\\n\",\n    \"               [{'secondary_y': False}, {'secondary_y': False}]]\\n\",\n    \"    )\\n\",\n    \"    \\n\",\n    \"    # 1. VBD by Round\\n\",\n    \"    round_vbd = draft_df.groupby('round')['vbd'].agg(['mean', 'std']).reset_index()\\n\",\n    \"    \\n\",\n    \"    fig.add_trace(\\n\",\n    \"        go.Scatter(\\n\",\n    \"            x=round_vbd['round'],\\n\",\n    \"            y=round_vbd['mean'],\\n\",\n    \"            mode='lines+markers',\\n\",\n    \"            name='Avg VBD',\\n\",\n    \"            line=dict(color='blue', width=3)\\n\",\n    \"        ),\\n\",\n    \"        row=1, col=1\\n\",\n    \"    )\\n\",\n    \"    \\n\",\n    \"    # 2. Position Distribution\\n\",\n    \"    pos_counts = draft_df['position'].value_counts()\\n\",\n    \"    \\n\",\n    \"    fig.add_trace(\\n\",\n    \"        go.Pie(\\n\",\n    \"            labels=pos_counts.index,\\n\",\n    \"            values=pos_counts.values,\\n\",\n    \"            name=\\\"Positions\\\"\\n\",\n    \"        ),\\n\",\n    \"        row=1, col=2\\n\",\n    \"    )\\n\",\n    \"    \\n\",\n    \"    # 3. Team Value Accumulation\\n\",\n    \"    team_cumulative = []\\n\",\n    \"    for team in config['team_names']:\\n\",\n    \"        team_picks = draft_df[draft_df['team'] == team].sort_values('pick')\\n\",\n    \"        cumulative_vbd = team_picks['vbd'].cumsum().tolist()\\n\",\n    \"        if cumulative_vbd:\\n\",\n    \"            fig.add_trace(\\n\",\n    \"                go.Scatter(\\n\",\n    \"                    x=list(range(1, len(cumulative_vbd) + 1)),\\n\",\n    \"                    y=cumulative_vbd,\\n\",\n    \"                    mode='lines',\\n\",\n    \"                    name=team,\\n\",\n    \"                    showlegend=False\\n\",\n    \"                ),\\n\",\n    \"                row=2, col=1\\n\",\n    \"            )\\n\",\n    \"    \\n\",\n    \"    # 4. Pick Value vs Expected (simplified)\\n\",\n    \"    # Calculate value gained for each pick\\n\",\n    \"    value_data = []\\n\",\n    \"    for _, pick in draft_df.iterrows():\\n\",\n    \"        player_data = cheat_sheet[cheat_sheet['Player'] == pick['player']]\\n\",\n    \"        if not player_data.empty:\\n\",\n    \"            expected_pick = player_data.iloc[0]['Draft_Rank']\\n\",\n    \"            actual_pick = pick['pick']\\n\",\n    \"            value_gained = expected_pick - actual_pick\\n\",\n    \"            value_data.append({\\n\",\n    \"                'pick': actual_pick,\\n\",\n    \"                'value_gained': value_gained,\\n\",\n    \"                'team': pick['team']\\n\",\n    \"            })\\n\",\n    \"    \\n\",\n    \"    if value_data:\\n\",\n    \"        value_df = pd.DataFrame(value_data)\\n\",\n    \"        fig.add_trace(\\n\",\n    \"            go.Scatter(\\n\",\n    \"                x=value_df['pick'],\\n\",\n    \"                y=value_df['value_gained'],\\n\",\n    \"                mode='markers',\\n\",\n    \"                marker=dict(\\n\",\n    \"                    size=8,\\n\",\n    \"                    color=value_df['value_gained'],\\n\",\n    \"                    colorscale='RdYlGn',\\n\",\n    \"                    showscale=True\\n\",\n    \"                ),\\n\",\n    \"                name='Value Gained',\\n\",\n    \"                showlegend=False\\