    "class PostDraftAnalyzer:\n",
    "    def __init__(self, config, cheat_sheet):\n",
    "        self.config = config\n",
    "        self.cheat_sheet = cheat_sheet\n",
    "        self.draft_data = None\n",
    "        self.team_rosters = None\n",
    "        \n",
    "    def load_draft_results(self, filename='draft_state.pkl'):\n",
    "        \"\"\"Load completed draft results\"\"\"\n",
    "        try:\n",
    "            with open(filename, 'rb') as f:\n",
    "                state = pickle.load(f)\n",
    "            \n",
    "            self.draft_data = state['draft_history']\n",
    "            self.team_rosters = state['team_rosters']\n",
    "            print(f\"‚úÖ Loaded draft with {len(self.draft_data)} picks\")\n",
    "            return True\n",
    "        except FileNotFoundError:\n",
    "            print(f\"‚ùå Draft file {filename} not found\")\n",
    "            return False\n",
    "    \n",
    "    def analyze_team(self, team_name):\n",
    "        \"\"\"Comprehensive team analysis\"\"\"\n",
    "        if not self.team_rosters or team_name not in self.team_rosters:\n",
    "            print(f\"‚ùå Team {team_name} not found\")\n",
    "            return None\n",
    "        \n",
    "        roster = self.team_rosters[team_name]\n",
    "        if not roster:\n",
    "            print(f\"‚ùå No players found for {team_name}\")\n",
    "            return None\n",
    "        \n",
    "        analysis = {\n",
    "            'team_name': team_name,\n",
    "            'total_players': len(roster),\n",
    "            'total_vbd': sum(p['Custom_VBD'] for p in roster),\n",
    "            'avg_vbd': np.mean([p['Custom_VBD'] for p in roster]),\n",
    "            'position_breakdown': {},\n",
    "            'round_analysis': {},\n",
    "            'value_analysis': {},\n",
    "            'strengths': [],\n",
    "            'weaknesses': [],\n",
    "            'grade': 'B'\n",
    "        }\n",
    "        \n",
    "        # Position breakdown\n",
    "        for player in roster:\n",
    "            pos = player['Position']\n",
    "            if pos not in analysis['position_breakdown']:\n",
    "                analysis['position_breakdown'][pos] = {\n",
    "                    'count': 0,\n",
    "                    'total_vbd': 0,\n",
    "                    'players': []\n",
    "                }\n",
    "            \n",
    "            analysis['position_breakdown'][pos]['count'] += 1\n",
    "            analysis['position_breakdown'][pos]['total_vbd'] += player['Custom_VBD']\n",
    "            analysis['position_breakdown'][pos]['players'].append(player)\n",
    "        \n",
    "        # Round analysis\n",
    "        for player in roster:\n",
    "            round_num = player.get('Round', 0)\n",
    "            if round_num not in analysis['round_analysis']:\n",
    "                analysis['round_analysis'][round_num] = []\n",
    "            analysis['round_analysis'][round_num].append(player)\n",
    "        \n",
    "        # Value analysis - compare actual vs expected\n",
    "        value_gained = 0\n",
    "        for player in roster:\n",
    "            actual_pick = player.get('Pick_Number', 0)\n",
    "            expected_pick = player.get('Draft_Rank', actual_pick)\n",
    "            value_gained += expected_pick - actual_pick  # Positive = got player later than expected\n",
    "        \n",
    "        analysis['value_analysis']['total_value_gained'] = value_gained\n",
    "        analysis['value_analysis']['avg_value_per_pick'] = value_gained / len(roster) if roster else 0\n",
    "        \n",
    "        # Identify strengths and weaknesses\n",
    "        self._assess_team_strength(analysis)\n",
    "        \n",
    "        # Calculate grade\n",
    "        analysis['grade'] = self._calculate_team_grade(analysis)\n",
    "        \n",
    "        return analysis\n",
    "    \n",
    "    def _assess_team_strength(self, analysis):\n",
    "        \"\"\"Assess team strengths and weaknesses\"\"\"\n",
    "        pos_breakdown = analysis['position_breakdown']\n",
    "        required_slots = self.config['roster']['roster_slots']\n",
    "        \n",
    "        # Check position coverage\n",
    "        for pos, required in required_slots.items():\n",
    "            if pos == 'FLEX':\n",
    "                continue\n",
    "            \n",
    "            current = pos_breakdown.get(pos, {'count': 0})['count']\n",
    "            \n",
    "            if current >= required * 1.5:  # 50% more than required\n",
    "                analysis['strengths'].append(f\"Deep at {pos} ({current} players)\")\n",
    "            elif current < required:\n",
    "                analysis['weaknesses'].append(f\"Light at {pos} ({current}/{required} required)\")\n",
    "        \n",
    "        # Check VBD concentration\n",
    "        total_vbd = analysis['total_vbd']\n",
    "        if total_vbd > 400:  # High total value\n",
    "            analysis['strengths'].append(f\"High total value ({total_vbd:.1f} VBD)\")\n",
    "        elif total_vbd < 250:\n",
    "            analysis['weaknesses'].append(f\"Lower total value ({total_vbd:.1f} VBD)\")\n",
    "        \n",
    "        # Check early round success\n",
    "        early_picks = [p for p in analysis['round_analysis'].get(1, []) + \n",
    "                      analysis['round_analysis'].get(2, [])]\n",
    "        if early_picks:\n",
    "            early_vbd = sum(p['Custom_VBD'] for p in early_picks)\n",
    "            if early_vbd > 150:\n",
    "                analysis['strengths'].append(\"Strong early rounds\")\n",
    "            elif early_vbd < 80:\n",
    "                analysis['weaknesses'].append(\"Weak early rounds\")\n",
    "    \n",
    "    def _calculate_team_grade(self, analysis):\n",
    "        \"\"\"Calculate letter grade for team\"\"\"\n",
    "        score = 0\n",
    "        \n",
    "        # VBD score (40% of grade)\n",
    "        vbd_score = min(100, analysis['total_vbd'] / 4)  # Scale to 100\n",
    "        score += vbd_score * 0.4\n",
    "        \n",
    "        # Value gained score (30% of grade)\n",
    "        value_gained = analysis['value_analysis']['total_value_gained']\n",
    "        value_score = min(100, max(0, (value_gained + 50) * 2))  # Center around 0, scale\n",
    "        score += value_score * 0.3\n",
    "        \n",
    "        # Position balance score (30% of grade)\n",
    "        balance_score = 100 - len(analysis['weaknesses']) * 15  # Subtract for weaknesses\n",
    "        balance_score = max(0, balance_score)\n",
    "        score += balance_score * 0.3\n",
    "        \n",
    "        # Convert to letter grade\n",
    "        if score >= 90:\n",
    "            return 'A+'\n",
    "        elif score >= 85:\n",
    "            return 'A'\n",
    "        elif score >= 80:\n",
    "            return 'A-'\n",
    "        elif score >= 75:\n",
    "            return 'B+'\n",
    "        elif score >= 70:\n",
    "            return 'B'\n",
    "        elif score >= 65:\n",
    "            return 'B-'\n",
    "        elif score >= 60:\n",
    "            return 'C+'\n",
    "        elif score >= 55:\n",
    "            return 'C'\n",
    "        else:\n",
    "            return 'C-'\n",
    "    \n",
    "    def compare_all_teams(self):\n",
    "        \"\"\"Compare all teams in the league\"\"\"\n",
    "        if not self.team_rosters:\n",
    "            print(\"‚ùå No draft data loaded\")\n",
    "            return None\n",
    "        \n",
    "        team_comparisons = []\n",
    "        \n",
    "        for team_name in self.team_rosters.keys():\n",
    "            analysis = self.analyze_team(team_name)\n",
    "            if analysis:\n",
    "                team_comparisons.append({\n",
    "                    'Team': team_name,\n",
    "                    'Grade': analysis['grade'],\n",
    "                    'Total_VBD': analysis['total_vbd'],\n",
    "                    'Avg_VBD': analysis['avg_vbd'],\n",
    "                    'Value_Gained': analysis['value_analysis']['total_value_gained'],\n",
    "                    'Strengths': len(analysis['strengths']),\n",
    "                    'Weaknesses': len(analysis['weaknesses'])\n",
    "                })\n",
    "        \n",
    "        return pd.DataFrame(team_comparisons).sort_values('Total_VBD', ascending=False)\n",
    "    \n",
    "    def generate_trade_recommendations(self, team_name):\n",
    "        \"\"\"Generate trade recommendations for a team\"\"\"\n",
    "        team_analysis = self.analyze_team(team_name)\n",
    "        if not team_analysis:\n",
    "            return []\n",
    "        \n",
    "        recommendations = []\n",
    "        pos_breakdown = team_analysis['position_breakdown']\n",
    "        \n",
    "        # Find surplus positions\n",
    "        surplus_positions = []\n",
    "        for pos, data in pos_breakdown.items():\n",
    "            required = self.config['roster']['roster_slots'].get(pos, 1)\n",
    "            if data['count'] > required * 1.5:  # Significant surplus\n",
    "                surplus_positions.append(pos)\n",
    "        \n",
    "        # Find deficit positions  \n",
    "        deficit_positions = []\n",
    "        for weakness in team_analysis['weaknesses']:\n",
    "            if 'Light at' in weakness:\n",
    "                pos = weakness.split('Light at ')[1].split(' ')[0]\n",
    "                deficit_positions.append(pos)\n",
    "        \n",
    "        # Generate trade suggestions\n",
    "        if surplus_positions and deficit_positions:\n",
    "            recommendations.append(f\"Trade surplus {surplus_positions[0]} for {deficit_positions[0]} depth\")\n",
    "        \n",
    "        if 'QB' in surplus_positions:\n",
    "            recommendations.append(\"Trade backup QB for skill position player\")\n",
    "        \n",
    "        return recommendations\n",
    "    \n",
    "    def suggest_waiver_targets(self, team_name, top_n=10):\n",
    "        \"\"\"Suggest waiver wire targets based on team needs\"\"\"\n",
    "        team_analysis = self.analyze_team(team_name)\n",
    "        if not team_analysis:\n",
    "            return []\n",
    "        \n",
    "        # Find undrafted players\n",
    "        drafted_players = set()\n",
    "        for roster in self.team_rosters.values():\n",
    "            for player in roster:\n",
    "                drafted_players.add(player['Player'])\n",
    "        \n",
    "        undrafted = self.cheat_sheet[~self.cheat_sheet['Player'].isin(drafted_players)]\n",
    "        \n",
    "        # Prioritize by team needs\n",
    "        need_positions = []\n",
    "        for weakness in team_analysis['weaknesses']:\n",
    "            if 'Light at' in weakness:\n",
    "                pos = weakness.split('Light at ')[1].split(' ')[0]\n",
    "                need_positions.append(pos)\n",
    "        \n",
    "        if need_positions:\n",
    "            priority_targets = undrafted[undrafted['Position'].isin(need_positions)]\n",
    "            other_targets = undrafted[~undrafted['Position'].isin(need_positions)]\n",
    "            \n",
    "            targets = pd.concat([priority_targets, other_targets]).head(top_n)\n",
    "        else:\n",
    "            targets = undrafted.head(top_n)\n",
    "        \n",
    "        return targets[['Player', 'Position', 'Team', 'Custom_VBD']]\n",
    "\n",
    "# Initialize analyzer\n",
    "analyzer = PostDraftAnalyzer(config, cheat_sheet)\n",
    "print(\"\\n‚ö° Post-Draft Analyzer ready!\")"
   ]
